
public class bubblesort {

	public static void main(String[] args) {
		int[] arr = { 9, 7, 6, 8, 4, 3, 2, 1 };

		for (int i = 0; i < arr.length - 1; i++) {
			
			for (int n = 0; n < arr.length - 1; n++) {
				if (arr[n] > arr[n + 1]) {
					int swap = arr[n];
					arr[n] = arr[n + 1];
					arr[n + 1] = swap;
					System.out.print(n);

				}
			}

		}

	}

}
